<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Comanda"
           table="Comanda">
        <id name="id" column="ID_comanda">
        </id>
        <property
                name="client"
                column="Client"/>
        <property
                name="date"
                column="Data"/>
        <property
                name="suma"
                column="Suma"/>
    </class>
</hibernate-mapping>