<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Produs"
           table="Produs">
        <id name="id" column="ID_produs">
        </id>
        <property
                name="denumire"
                column="Denumire"/>
        <property
                name="pret"
                column="Pret"/>
        <property
                name="cantitate"
                column="Cantitate"/>
    </class>
</hibernate-mapping>